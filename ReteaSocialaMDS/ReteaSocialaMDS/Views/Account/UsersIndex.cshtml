
@{
    ViewBag.Title = "UsersIndex";
}

<h2>UsersIndex</h2>

<div class="row">
    <div class="panel panel-primary">
        <div class="panel-heading">Users emails index</div>
        <div class="panel-body">
            <p>
                The list of users defined in Retea Sociala MDS
            </p>
        </div>
        <ul class="list-group">
            @foreach (string userName in Model)
            {
                <li class="list-group-item">
                    @userName
                </li>
            }
        </ul>
    </div>

</div>